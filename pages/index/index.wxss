image {
    width: 100%;
}

.shop_info {
    margin-top: 5rpx;
}

.shop_wrap {
    display: flex;
    flex-wrap: wrap;
    justify-items: center;
    align-content: center;
}

.shop_count {
    flex-wrap: wrap;
    justify-items: center;
    align-content: center;
    text-align: center;
    flex: 1;
    padding: 20rpx;
    /* background-color: #ccc; */
    /* display: flex; */
}

.shop_count image {
    width: 100%;
}


